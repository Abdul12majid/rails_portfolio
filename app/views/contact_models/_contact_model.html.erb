<div id="<%= dom_id contact_model %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= contact_model.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Email:</strong>
    <%= contact_model.email %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Message:</strong>
    <%= contact_model.message %>
  </p>

</div>
